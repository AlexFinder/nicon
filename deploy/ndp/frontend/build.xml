<project>
    <!--property 相对与basedir，include、import相对于当前文件位置-->
    <property file="./deploy/ndp/frontend/build.properties"/>
    <import file="./tasks.xml"/>

    <target name="deploy">
        <echo message="begin deploy......"/>
        <!--删除compressed-->
        <antcall target="clean"/>

        <!--拷贝到部署文件夹-->
        <antcall target="cp"/>

        <echo message="全部优化后总耗时为："/>
    </target>
</project>
